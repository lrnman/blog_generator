<%- partial('_partial/head') %>
<body class="<%- is_home() && page.current == 1? 'is-loading hide': 'loading' %>" id="top">

<% if (is_home() && page.current == 1) { %>
 <%- partial('_partial/banner') %>
<% } %>

 <div id="wrapper" class="fade-in">
  <!-- Intro -->
  <% if (is_home() && page.current == 1) {%>
  <div id="intro">
   <h1><%- theme['intro_h1'] %></h1>
   <p><%- theme['intro_p'] %></p>
   <ul class="actions">
    <li><a href="#header" class="button icon solo fa-arrow-down scrolly">Continue</a></li>
   </ul>
  </div>
  <% } %>
  <!-- Header -->
  <%- partial('_partial/header') %>

  <!-- Nav -->
  <%- partial('_partial/nav') %>

  <!-- Main -->
  <div id="main">
    <%- body %>
  </div>


  <!-- Nav -->
  <%- partial('_partial/footer') %>
  <div class="bg" style="
          background-image:		url('/images/overlay.png'),	linear-gradient(0deg, rgba(0,0,0,0.1), rgba(0,0,0,0.1)),	url('/images/<%- theme.bg %>');
          background-position:	center,								center,														<%- theme.bg_position %>;
          "></div>
 </div>
 <%- theme.scripts ? js(theme.scripts) : '' %>
</body>
</html>