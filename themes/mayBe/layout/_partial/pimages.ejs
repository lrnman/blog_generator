<div class="box alt p-images-box"  data-trueorder="true" data-col="<%- col ? col : photos.length < 5 ? 2 : col %>">
    <div class="poptrox macy-container" style="-moz-column-count:<%- col ? col : photos.length < 5 ? 2 : col %>;
            -webkit-column-count:<%- col ? col : photos.length < 5 ? 2 : col %>;
            column-count:<%- col ? col : photos.length < 5 ? 2 : col %>;">
        <% for (var i in photos) { %>
            <a class="noevent" href="<%- photos[i] %>" ><img src="<%- photos[i] %>" alt=""></a>
        <% } %>
    </div>
</div>